    "web-vitals": "^2.1.4"
  },
  "private": false,
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "test": "jest ./src/__tests__",
    "check-types": "tsc --noEmit --project tsconfig.json",
    "prepare": "husky install",
    "coverage": "jest --coverage",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "lint:ci": "eslint src/ --format junit --output-file ./reports/eslint.xml",
    "prettify": "prettier \"src/**/*.{js,ts,tsx,jsx,css,graphql}\" --write --log-level=warn",
    "prettify:check": "prettier \"src/**/*.{js,ts,tsx,jsx,css,graphql}\" --check --log-level=warn",
    "lp": "pnpm lint --fix && pnpm prettify"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/preset-env": "^7.22.9",
    "@babel/preset-react": "^7.22.5",
    "@commitlint/cli": "^17.6.7",
